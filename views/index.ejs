<body>
    <div class="container">
        <h1 class="text-center">Popular Photo</h1>
        <div class="row row-cols-1 row-cols-md-3 g-4 py-5">
            <% if(Post != null && Post.length > 0) { %>
                <% for(let i = 0; i < Post.length; i++) { %>
                    <!-- <p class="alert alert-danger">
                        <%= Post[i] %>
                    </p> -->
                    <div class="col">
                        <div class="card">
                            <img src="<%=Post[i].filePath %>" class="card-img-top" alt="...">
                            <div class="card-body">
                                <h5 class="card-title"><%=Post[i].title %></h5>
                                <p class="card-text"><%=Post[i].description %></p>
                            </div>
                            <div class="mb-5 d-flex justify-content-around">
                                <% if(Post[i].price != null ) {%>
                                <h3><%=Post[i].price%> THB</h3>
                                <% } %>
                                <% if (Post[i].price == null) { %>
                                    <h3> Free </h3>
                                    <%}%>
                                <%if (loggedIn){%>
                                    <% if(Post[i].UserID != UserData.UserID && Post[i].OwnerID != UserData.UserID) { %>
                                        <form action="/buy/<%=Post[i]._id %>" method="post">
                                            <button type="submit" class="btn btn-primary">Buy Now</button>
                                        </form>
                                    <% } %>
                                <% } %>
                            </div>
                        </div>
                    </div>
                <% } %>
            <% } %>

